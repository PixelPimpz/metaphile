#!/usr/bin/env bash
LOCAL_ROOT="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
tmux set -g '@METAPHILEROOT' "$LOCAL_ROOT"
ICONS="$(tmux show -gqv @ICONS)"
SHARE="$( tmux show -gqv @CHER )"
source "$SHARE/dump.fun"
source "$SHARE/fatal.fun"
tmux source "$LOCAL_ROOT/conf/hooks.conf"
tmux source "$LOCAL_ROOT/conf/keys.conf"
dump ">>> welcome adventurer!"
tmux bind M-m run-shell "$LOCAL_ROOT/scripts/metaphile.sh"
tmux run-shell "$LOCAL_ROOT/scripts/metaphile.sh"
